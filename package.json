{
  "name": "openmined-website",
  "homepage": "https://www.openmined.org",
  "version": "1.0.0",
  "private": false,
  "license": "Apache-2.0",
  "dependencies": {
    "classnames": "^2.2.6",
    "connected-react-router": "^6.2.2",
    "intersection-observer": "0.5.1",
    "mapbox-gl": "0.52.0",
    "moment": "2.24.0",
    "node-sass": "^4.12.0",
    "normalize.css": "^8.0.1",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-fullstory": "^1.0.3",
    "react-helmet": "^5.2.0",
    "react-redux": "6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.2.2",
    "react-scripts": "2.1.3",
    "react-scroll-percentage": "3.1.0",
    "react-tilt": "^0.1.4",
    "redux": "4.0.1",
    "redux-thunk": "^2.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "yarn build && yarn deploy:s3 && yarn deploy:cloudfront",
    "deploy:s3": "aws s3 sync build/ s3://openmined.org --cache-control max-age=172800 --delete --profile openmined",
    "deploy:cloudfront": "aws configure set preview.cloudfront true && aws cloudfront create-invalidation --distribution-id ERTRGINZVVHN8 --paths /index.html --profile openmined"
  },
  "devDependencies": {
    "husky": "1.3.1",
    "prettier": "1.16.1",
    "pretty-quick": "1.10.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
