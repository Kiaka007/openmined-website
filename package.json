{
  "name": "openmined-website",
  "homepage": "https://openmined.org",
  "version": "0.1.0",
  "private": false,
  "license": "Apache-2.0",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "create-react-server": "^0.3.1",
    "es6-promise": "^4.2.4",
    "express": "^4.16.2",
    "flow-bin": "^0.57.3",
    "ignore-styles": "^5.0.1",
    "isomorphic-fetch": "^2.2.1",
    "lint-staged": "^4.1.3",
    "moment": "^2.19.3",
    "node-sass-chokidar": "^0.0.3",
    "normalize.css": "^7.0.0",
    "npm-run-all": "^4.1.1",
    "openmined-ui": "^0.0.5",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.6",
    "react-render-html": "^0.6.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.14",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": ["prettier --single-quote --write", "git add"]
  },
  "scripts": {
    "precommit": "lint-staged",
    "build:css":
      "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch:css":
      "yarn build:css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start:js": "react-scripts start",
    "build": "yarn build:css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "yarn lint:js",
    "lint:js": "eslint .",
    "start": "npm-run-all -p watch:css start:js",
    "serve": "NODE_ENV=production node ./server/index.js",
    "deploy": "npm run deploy:ecs",
    "deploy:ecs":
      "npm run docker:build && npm run docker:tag:ecs && npm run docker:push:ecs",
    "docker:build": "docker build -t openmined-website .",
    "docker:tag:ecs":
      "docker tag openmined-website:latest 273848247695.dkr.ecr.us-east-1.amazonaws.com/openmined-website:latest",
    "docker:push:ecs":
      "docker push 273848247695.dkr.ecr.us-east-1.amazonaws.com/openmined-website:latest",
    "ecs:refresh": "aws ecr get-login --no-include-email --region us-east-1"
  },
  "devDependencies": {
    "eslint": "^4.10.0",
    "eslint-config-prettier": "^2.7.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "prettier": "^1.7.4"
  }
}
